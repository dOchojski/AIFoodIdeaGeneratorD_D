<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" spacing="20.0" styleClass="vbox" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.d_d.aifoodideageneratord_d.controller.RecommendationWindowController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="recommendationLabel" wrapText="true"/>
    <Button fx:id="saveReceiptButton" text="Zapisz przepis" onAction="#handleSaveRecipe"/>
    <Button fx:id="exportToPdfButton" text="Eksportuj do PDF" onAction="#handleExportToPdf"/>

</VBox>
